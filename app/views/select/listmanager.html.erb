<h3 style="margin-left:160px;">Listing Managers</h3>



<table class="centered highlight bordered">
       <thead>
         <tr>
           <th>Manager's Id</th>
             <th>Email</th>
             <th>Name</th>

         </tr>
       </thead>

       <tbody>
         <% @users.each do |user| %>
         <% if @users.nil? %>
         <% return "There is no result to show."  %>
         <% else %>
         <% if user.ismanager==true%>
         <tr>
           <td><%= user.id %></td>
           <td><%= user.try(:fullname) %></td>
           <td><%= user.try(:email) %></td>
         </tr>
         <% else %>

         <% end %>

         <% end %>

         <% end %>
       </tbody>
     </table>
